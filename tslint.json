{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "interface-name": [
    true,
    "never-prefix"
  ],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "ordered-imports": false
  }
}