{
  "compilerOptions": {
    "outDir": "./www/out",
    "allowJs": true,
    "target": "es5",
    "normalTypeScriptOptions": "here",
    "plugins": [
      {
        "transform": "typescript-is/lib/transform-inline/transformer"
      }
    ]
  },
  "include": [
    "./www/src/*",
    "./www/src/events/*"
  ],
  "typedocOptions": {
    "mode": "file",
    "out": "docs"
  }
}